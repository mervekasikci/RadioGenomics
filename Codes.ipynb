  names(genomics_rfe)[1] <- "Histology"
  
  all_rfe <- cbind(radiomics_rfe, genomics_rfe[,-1])
  
  
  radiomics_rfe$Histology <- plyr::revalue(radiomics_rfe$Histology, 
                                           c("1" = "Adenocarcinoma", 
                                             "0" = "Squamous_cell_carcinoma"))
  
